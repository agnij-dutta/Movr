{
  "name": "apm-cli",
  "version": "1.0.0",
  "description": "Aptos Package Manager - A CLI tool for managing Aptos Move packages with IPFS integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "movr": "dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/bin/cli.ts",
    "start": "node dist/bin/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "aptos",
    "blockchain",
    "cli",
    "package-manager",
    "ipfs",
    "move"
  ],
  "author": "APM Team",
  "license": "MIT",
  "dependencies": {
    "@aptos-labs/ts-sdk": "^2.0.0",
    "@types/figlet": "^1.7.0",
    "archiver": "^6.0.1",
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.3.1",
    "extract-zip": "^2.0.1",
    "figlet": "^1.8.1",
    "form-data": "^4.0.0",
    "formdata-node": "^6.0.3",
    "fs-extra": "^11.2.0",
    "fuse.js": "^7.1.0",
    "inquirer": "^9.2.16",
    "joi": "^17.11.0",
    "jszip": "^3.10.1",
    "ora": "^7.0.1",
    "pinata": "^2.4.9",
    "winston": "^3.12.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/extract-zip": "^2.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
