{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "cli/**",
          "!cli/node_modules/.cache/**"
        ]
      }
    }
  ],
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
} 