{
  "name": "movr-api",
  "version": "1.0.0",
  "description": "Next.js API backend for movr CLI",
  "scripts": {
    "dev": "next dev -p 3001",
    "prebuild": "npm run copy-cli",
    "build": "next build",
    "copy-cli": "mkdir -p cli && cp -r ../cli/dist cli/ && cp -r ../cli/node_modules cli/ 2>/dev/null || true",
    "start": "next start -p 3001",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "cors": "^2.8.5",
    "child_process": "^1.0.2",
    "@aptos-labs/ts-sdk": "^2.0.0",
    "archiver": "^6.0.1",
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.3.1",
    "extract-zip": "^2.0.1",
    "figlet": "^1.8.1",
    "form-data": "^4.0.0",
    "formdata-node": "^6.0.3",
    "fs-extra": "^11.2.0",
    "fuse.js": "^7.1.0",
    "inquirer": "^9.2.16",
    "joi": "^17.11.0",
    "ora": "^7.0.1",
    "pinata": "^2.4.9",
    "winston": "^3.12.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/archiver": "^6.0.3",
    "@types/extract-zip": "^2.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 